	image_rock = load(\"res://assets/meshlibs/%sRock.tres\" % type)
	texture_rock.create_from_image(image_rock, 0)
	meshlib.set_item_preview(1, texture_rock)
	ResourceSaver.save(\"res://assets/meshlibs/%s.tres\" % type, meshlib)
